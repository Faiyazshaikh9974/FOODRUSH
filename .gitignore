# Ignore node_modules in both frontend and backend
frontend/node_modules/
backend/node_modules/

# If there's also a root node_modules (e.g., if you ran npm install at the root)
node_modules/

# Ignore build output (Parcel typically outputs to 'dist' by default)
frontend/dist/
backend/dist/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Parcel cache (used to speed up rebuilds, but shouldn't be committed)
.parcel-cache/
frontend/.parcel-cache/
backend/.parcel-cache/

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# macOS system files
.DS_Store

# Optional OS / IDE / Editor stuff
.vscode/
.idea/
*.swp
